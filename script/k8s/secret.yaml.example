# Make sure to change the name of this file to secret.yaml
# IMPORTANT: DO NOT COMMIT THIS FILE IF IT CONTAINS ACTUAL SECRETS FOR TESTING
apiVersion: v1
kind: Secret
metadata:
  name: errbot-secret
  namespace: errbot
type: Opaque
data:
  # For all of the lines below, replace ${KEY} with the base64 encoded value of the secret
  # EXAMPLE: "asdfasdf=="
  CHAT_SERVICE_TOKEN: "${CHAT_SERVICE_TOKEN}"
  RIOT_TOKEN: "${RIOT_TOKEN}"
  AWS_ACCESS_KEY_ID: "${AWS_ACCESS_KEY_ID}"
  AWS_SECRET_ACCESS_KEY: "${AWS_SECRET_ACCESS_KEY}"
  SPOTIFY_CLIENT_ID: "${SPOTIFY_CLIENT_ID}"
  SPOTIFY_CLIENT_SECRET: "${SPOTIFY_CLIENT_SECRET}"
  SENTRY: "${SENTRY}"
